package project11;

public class mainClass {

	public static void main(String[] args) {
		
		int num1, num2 ;
		
		num1 = 128;
		
		num2 = 56;
		
		num1 = (num1 & num2) | (num1 & num2);
		num2 = ~(num1^num2);
		
		System.out.println("num1 = "+ num1);
		System.out.println("num2 = "+ num2);
		
		

	}

}
