package project13;

public class mainClass {

	public static void main(String[] args) {
		
		
		int a, b, c, d, e, f ;
		
		a = (int)(Math.random()*100);
		
		System.out.println("0~99 랜덤 숫자 = " + a);
		
		b = ((int)(Math.random() * 5)+11) ;
		
		System.out.println("11~15 랜덤 숫자 = " + b);
		
		

	}

}
