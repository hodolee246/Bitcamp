
public class mainClass {

	public static void main(String[] args) {
	
		/*
		
			loop
			
			for(초기화;조건식;연산식
			{
				처리
			}
			초기화
			while(조건식)
			{
			처리
			연산식
			
			do
			{
				처리
				연산식
			}while(조건)
		*/
		
		int w;
		
		w = 0;
		while(w < 10)
		{
			System.out.println("while w:" + w);
			
			w++;
		}
	}

}
