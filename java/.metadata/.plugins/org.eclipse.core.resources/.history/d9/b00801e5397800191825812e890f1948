package work3;

public class mainClass {

	public static void main(String[] args) {
		/*
		
		5x9
		column	row
		열		행

		
		for(int j=0; j<9; j++) {
			
			for(int i=0; i<5; i++) {
			
				System.out.print("*"); // 행을 만들어주는 for문
		}
		System.out.println();	
	}
		
		*/
		int len = 1;
		
		for(int j=0; j<9; j++) {
			if(j < 5) len++;
			for(int i=0; i<len; i++) {
			
				System.out.print("*");
		}
		System.out.println();	
	}
		
		
		
	}

}
