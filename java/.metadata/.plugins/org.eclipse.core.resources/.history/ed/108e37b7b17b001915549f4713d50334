		// 스페이드, 다이아, 하트, 클로버
		// 0~12    13~25 26~38 39~51
		/*
		 	랜덤 숫자 	카드 숫자 	그림
		 	13		1		다이아
		 	51		13		클로버
		 */
		int array[][] = new int[4][13];
		int temp;
		for (int i = 0; i < array.length; i++) {
			
			for (int j = 0; j < array[0].length; j++) {
				array[i][j] = array[i][j];
			}
		}
		
