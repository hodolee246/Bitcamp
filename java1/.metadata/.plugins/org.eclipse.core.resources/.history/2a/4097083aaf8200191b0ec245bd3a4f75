package main;

import java.util.Scanner;

import dao.DaoClass;

public class mainClass {

	public static void main(String[] args) {
		
		
		DaoClass dao = new DaoClass();

		
		// menu

		/*
		
		1. 학생정보를 추가
		2. 학생정보를 삭제한다
		3. 학생정보를 검색
		4. 학생정보를 수정
		5. 학생정보를 모두 출력한다.
		*/
		dao.insert();
		dao.delete();
		dao.insert();
	}

}
