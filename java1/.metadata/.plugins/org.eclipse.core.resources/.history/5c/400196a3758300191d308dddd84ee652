package marine;

import main.Unit;

public class Marine extends Unit {

	int x, y ;
	
	public Marine() {
		
	}
	
	public void move(int x, int y) {
		this.x = x;
		this.y = y;
		System.out.println("지정된 위치로 이동" + x + "" + y );
	}
	
	public void stop() {
		
		System.out.println("현재 위치에 정지합니다.");
		
	}
	
	public void stimPack() {
		
		System.out.println("스팀팩을 사용한다.");
	}
}
