package parent;

public class Parent {
	
	private int number;

	public void Parent() {
		
		System.out.println("Parent Parent()");
		
	}
	
	public void Parent(int number) {
		
		// this();
		this.number = number;
		
		System.out.println("Parent Parent(int number)");
	}

	public int getNumber() {
		return number;
	}

	public void setNumber(int number) {
		this.number = number;
	}
	
	
}
