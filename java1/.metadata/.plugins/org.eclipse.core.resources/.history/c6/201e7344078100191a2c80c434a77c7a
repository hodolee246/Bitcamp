
public class mianClass {

	public static void main(String[] args) {
	
		
		String str = "홍길동-2001/03/14-서울시";
		
		String split[] = str.split("-");
		
		for (int i = 0; i < split.length; i++) {
			
			System.out.println(split[i]);
		}
		
		
		
		

		
		
		
		
		
		
		
		
		
		
	}

	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
}



















































