package hcls;

import single.SingletonClass;

public class HeClass {

	private int hnum;
	private String hname;
	
	public HeClass() {
		name = "전인우";
	}
	
	public void method() {
		SingletonClass sc = SingletonClass.getInstance();
		hnum = sc.num;
		
	}
	
	public void Function() {
		
		SingletonClass sc = SingletonClass.getInstance();
		sc.name = hname;
		
	}

	@Override
	public String toString() {
		return "HeClass [hnum=" + hnum + ", name=" + hname + "]";
	}
	
}
