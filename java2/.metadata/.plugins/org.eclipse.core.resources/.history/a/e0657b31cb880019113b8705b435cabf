package ycls;

import single.SingletonClass;

public class YouClass {

	
	private int younum;
	
	public YouClass() {
		
	}

	@Override
	public String toString() {
		return "YouClass [younum=" + younum + "]";
	}

	public int getYounum() {
		return younum;
	}
	
	public void method() {
		SingletonClass sc = SingletonClass.getInstance();
		younum = sc.num;
		
	}

	public void setYounum(int younum) {
		this.younum = younum;
	}
	
}
