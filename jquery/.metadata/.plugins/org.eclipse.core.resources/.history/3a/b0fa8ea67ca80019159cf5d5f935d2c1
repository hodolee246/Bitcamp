<script type="text/javascript">

$(function () {
	$("#q20").addClass("qq20");
	var abc = $(".eee").children().length;
	$("#q21").text(abc);

	$("li").click(function() {
		alert($(this).index() +1);
	});
	
	$("#btn").click(function() {
		
		if($("#t1").val() >= 76){
			$("#q23").children().remove();
			$("#q23").append("<p>합격</p>");	
		}else{
			$("#q23").children().remove();
			$("#q23").append("<p>불합격</p>");
	}
	});
	for (var i = 0; i < 4; i++) {
		
	var bbc = $(".q24").children().eq(i).text();
	$(".q25").prepend(bbc + ",");
	if(i==3){
		$(".q25").prepend(bbc + " ");
	}
	}
	
});
</script>

