$(function () {
	for (var i = 1; i < 101; i++) {
	if(i < 100){	
	$("#q27").before(i + ",");
	}else {
		$("#q27").before(i);
	}
	}
	var drink = {"커피", "홍차", "우유", "주스", "소다"};
	for (var i = 0; i < drink.length; i++) {
		$(".q28").append("<li>" + drink[i] +"</li>")''
		
	}
	
});
