<script type="text/javascript">
$(function () {
	$("#btn1").click(function () {	// 뒤에다가 추가한다.
//		$('p').append("<br> 추가(append) 요소 입니다.");
//		$('ol').append("<li>오렌지</li>")	// 생성될때마다 번호가 하나씩 증가하며 뒤에 생성된다.
		$('select').append("<option value = '스테이크'>스테이크</option>");
	});
	$("#btn2").click(function () {	// 앞에다가 추가한다.
//		$('p').prepend("<br> 추가(prepend) 요소 입니다.")
//		$('ol').prepend("<li>파인애플</li>")	// 생성될때마다 자동적으로 번호가 밀리면서 맨앞에 생성된다.
	
	});
});
</script>

